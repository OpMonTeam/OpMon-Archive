	rm -f $(OBJ_RELEASE) $(OUT_RELEASE)
	rm -rf exeLinux
	rm -rf $(OBJDIR_RELEASE)/src/regimys/playercore
	rm -rf $(OBJDIR_RELEASE)/src/regimys/start
	rm -rf $(OBJDIR_RELEASE)/src/regimys/save
	rm -rf $(OBJDIR_RELEASE)/src/regimys/objects/item
